{
  "name": "MeanieExpressSeed",
  "description": "Seed project for Express applications",
  "version": "1.0.0",
  "homepage": "https://github.com/meanie/express-seed",
  "author": {
    "name": "Adam Buczynski",
    "email": "me@adambuczynski.com",
    "url": "http://adambuczynski.com/"
  },
  "contributors": [],
  "license": "SEE LICENSE IN LICENSE.md",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/meanie/express-seed"
  },
  "bugs": {
    "url": "https://github.com/meanie/express-seed/issues"
  },
  "keywords": [],
  "engines": {
    "node": ">=6.2.0",
    "npm": ">=3.8.9"
  },
  "main": "scripts/server.js",
  "scripts": {
    "start": "node scripts/server.js",
    "dev": "nodemon scripts/server.js",
    "lint": "npm run lint:jshint -s && npm run lint:jscs -s",
    "lint:jshint": "jshint . --reporter=node_modules/jshint-stylish",
    "lint:jscs": "jscs . --reporter=node_modules/jscs-stylish",
    "pretest": "npm run lint -s",
    "istanbul": "babel-node ./node_modules/istanbul/lib/cli cover ./node_modules/mocha/bin/_mocha",
    "test": "npm run istanbul -s",
    "coverage": "open -a \"Google Chrome\" ./coverage/lcov-report/index.html",
    "posttest": "npm run coverage -s",
    "preversion": "npm run test -s",
    "postversion": "git push && git push --tags",
    "migrate": "node scripts/migrate.js",
    "refresh": "node scripts/migrate.js refresh",
    "deploy": "gcloud preview app deploy"
  },
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.1",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "entities": "^1.1.1",
    "express": "^4.13.4",
    "gcloud": "^0.34.0",
    "github": "^1.1.2",
    "glob": "^7.0.3",
    "i18n": "^0.6.0",
    "jsonwebtoken": "^7.0.0",
    "moment": "^2.13.0",
    "mongoose": "^4.4.19",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "nodemailer": "^2.1.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-strategy": "^1.0.0",
    "serve-static": "^1.10.3",
    "yargs": "^4.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "dirty-chai": "^1.2.2",
    "express-simulate-latency": "0.0.2",
    "istanbul": "^1.0.0-alpha.2",
    "jscs": "^3.0.3",
    "jscs-stylish": "^0.3.1",
    "jshint": "^2.9.2",
    "jshint-stylish": "^2.2.0",
    "mocha": "^2.5.3",
    "mocha-clean": "^1.0.0",
    "nodemon": "^1.9.2",
    "sinon": "^1.17.4",
    "sinon-mongoose": "^1.2.1"
  }
}
